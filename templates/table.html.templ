
<table class="min-w-full divide-y divide-gray-200 shadow overflow-hidden rounded-lg">
        <thead class="bg-gray-50">
            <tr>
                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                    ApiKey
                </th>
                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                    Schnittstelle
                </th>
                <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                    Beschreibung
                </th>
            </tr>
        </thead>
        <tbody id="table-body" class="bg-white divide-y divide-gray-200">
            <!-- Daten-Rows, generiert durch Ihren Server -->
            <tr>
                <td class="px-6 py-4 whitespace-nowrap">
                </td>
    
                <td class="px-6 py-4 whitespace-nowrap">
                    <span class="group-hover:opacity-100 transition-opacity bg-gray-800 px-1 text-sm text-gray-100 rounded-md absolute left-1/2 
    -translate-x-1/2 translate-y-full opacity-0 m-4 mx-auto">Currently not Implemented</span>
                    <select disabled class="z-10 bg-white divide-y bg-gray-200 focus:ring-blue-400 divide-gray-100 rounded-lg shadow p-1.5 dark:bg-gray-600" id="apitypein" name="apitype">
                    <option value="azure">Azure</option>
                    <option value="openai">OpenAI</option>
                <td class="px-6 py-4 whitespace-nowrap">
                    <input type="text" id="beschreibungin"  class="bg-gray-100 w-full border border-gray-300 text-gray-800 text-sm p-1.5 rounded-lg focus:ring-blue-400 focus:border-blue-300 block  dark:bg-gray-600 dark:border-gray-500 dark:placeholder-gray-200 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-300" name="beschreibung" />
                </td>
                </td>
                <td class="px-6 py-4 whitespace-nowrap text-right text-sm font-medium">

                    <button name="save"
                            type="submit"
                            id="save"
                            hx-post="/api2/table/entry/save"
                            hx-target="#popup-content"
                            hx-swap="innerHTML"
                            hx-include="[id='apikeyin'], [id='apitypein'], [id='beschreibungin']"
                            class="text-indigo-600 hover:text-indigo-900">
                            Save
                    </button>

                </td>
            </tr>
        {{ $length := len . }}
        {{ if gt $length 0}}
        {{ range . }}
        <tr>
            <td name="keyid" class="px-6 py-4 whitespace-nowrap">
                {{ .UUID }}
            </td>
            <td class="px-6 py-4 whitespace-nowrap">
                {{ .AiApi }}
            </td>
            <td class="px-6 py-4 whitespace-nowrap">
                {{ .Description }}
            </td>
            <td class="px-6 py-4 whitespace-nowrap text-right text-sm font-medium">
                                    <button name="delete"
                                            id="delete"
                                            hx-post="/api2/table/entry/delete/{{.UUID}}"
                                            hx-confirm="Möchtest du wirklich den Key Löschen?"
                                            class="text-red-600 hover:text-indigo-900">
                                            Delete
                                    </button>
            </td>
        </tr>
        {{ end }}
        {{ end }}
        <!-- Weitere Zeilen folgen je nach Daten -->
    </tbody>
</table>


